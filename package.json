{
  "name": "airswap",
  "description": "AirSwap CLI",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/airswap/airswap-maker-kit"
  },
  "contributors": [
    "Don Mosites <don.mosites@fluidity.io>"
  ],
  "bin": {
    "airswap": "./bin/run"
  },
  "bugs": "https://github.com/airswap/airswap-maker-kit/issues",
  "dependencies": {
    "@airswap/indexer": "3.6.8",
    "@airswap/order-utils": "0.3.19",
    "@airswap/swap": "4.3.5",
    "@airswap/tokens": "0.1.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^8.0.1",
    "@types/node-emoji": "^1.8.1",
    "@types/table": "^4.0.7",
    "axios": "^0.19.0",
    "cli-ux": "^5.4.1",
    "console.table": "^0.10.0",
    "ethers": "^4.0.40",
    "fs-extra": "^8.1.0",
    "jayson": "^3.2.0",
    "keytar": "^5.0.0",
    "node-emoji": "^1.10.0",
    "table": "^5.4.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "mock-require": "^3.0.3",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/airswap/airswap-maker-kit",
  "keywords": [
    "airswap"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "topics": {
      "account": {
        "description": "set, show, and unset the current account"
      },
      "balances": {
        "description": "display balances for the current account"
      },
      "intent": {
        "description": "get, set, and unset intents on the indexer"
      },
      "ip": {
        "description": "display local network addresses"
      },
      "network": {
        "description": "set the active ethereum network"
      },
      "orders": {
        "description": "get and take orders from a single peer or best available"
      },
      "quotes": {
        "description": "get quotes from a single peer or best available"
      },
      "tokens": {
        "description": "approve tokens for trade and update local metadata"
      }
    },
    "commands": "./src/commands",
    "bin": "airswap",
    "hooks": {
      "init": "./src/lib/init-hook"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
